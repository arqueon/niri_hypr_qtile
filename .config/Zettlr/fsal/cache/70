irlo en el intento.","header-includes":"\\usepackage{authblk}\n\\author{Rubén García Sánchez \\thanks{Correo: ruben.gsanchez@academicos.udg.mx, Orcid: 0000-0002-0007-3758}}\n\\affil{Universidad de Guadalajara}\n","date":"Enero de 2023","lang":"es-ES","reference-section-title":"Referencias","keywords":["microfascismos","fascismo","agenciamientos","racismo solo","sexismo","misoginia","homofobia","xenofobia","esfera pública digital","corrección política"],"toc":true,"pagesize":"letter","fontsize":"10pt","zotero":{"csl-style":"apa"}},"modified":false}]]